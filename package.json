{
	"private": false,
	"name": "banana-grid",
	"version": "1.4.7",
	"description": "",
	"main": "lib/main",
	"types": "@types/main",
	"scripts": {
		"link": "",
		"test": "jest --no-cache",
		"dev": "npm-run-all --parallel tsc-w webpack-dev-w webpack-prod-w && exit 0",
		"tsc-w": "tsc -w",
		"webpack-dev": "webpack --define process.env.NODE_ENV=development --config=webpack.dev.js --progress --color",
		"webpack-prod": "webpack --define process.env.NODE_ENV=production --config=webpack.prod.js --progress --color",
		"webpack-dev-w": "webpack -w --define process.env.NODE_ENV=development --config=webpack.dev.js --progress --color",
		"webpack-prod-w": "webpack -w --define process.env.NODE_ENV=production --config=webpack.prod.js --progress --color",
		"build": "yarn run clean && tsc && yarn run webpack-dev && yarn run webpack-prod && exit 0",
		"clean": "rimraf lib/* dist/* @types/* && exit 0",
		"clean-node": "rimraf package-lock.json node_modules/* && exit 0"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"react",
			"es2015"
		]
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest",
			"^.+\\.jsx?$": "babel-jest"
		},
		"testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dan2dev/banana-grid.git"
	},
	"author": {
		"name": "Dan C Castro"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dan2dev/banana-grid/issues"
	},
	"homepage": "https://github.com/dan2dev/banana-grid#readme",
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.39",
		"@types/jest": "^22.1.1",
		"awesome-typescript-loader": "^3.4.1",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-react": "^6.24.1",
		"clean-dir": "^1.0.3",
		"css-loader": "^0.28.9",
		"cssnano": "^3.10.0",
		"eslint": "^4.16.0",
		"eslint-config-standard": "^11.0.0-beta.0",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-node": "^5.2.1",
		"eslint-plugin-promise": "^3.6.0",
		"eslint-plugin-standard": "^3.0.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-webpack-plugin": "^2.30.1",
		"iisexpress-proxy": "^1.3.0",
		"jest": "^22.1.4",
		"node-sass": "^4.7.2",
		"optimize-css-assets-webpack-plugin": "^3.2.0",
		"regenerator-runtime": "^0.11.1",
		"rimraf": "^2.6.2",
		"sass-loader": "^6.0.6",
		"source-map-loader": "^0.2.3",
		"style-loader": "^0.20.1",
		"ts-jest": "^22.0.2",
		"typescript": "^2.7.1",
		"uglifyjs-webpack-plugin": "^1.1.8",
		"webpack": "^3.10.0",
		"webpack-livereload-plugin": "^1.0.0",
		"webpack-merge": "^4.1.1"
	},
	"dependencies": {
		"@babel/preset-env": "^7.0.0-beta.39",
		"@types/deep-equal": "^1.0.1",
		"@types/jquery": "^3.3.0",
		"@types/jsx-chai": "^3.0.16",
		"@types/lodash": "^4.14.98",
		"@types/node": "^9.4.0",
		"@types/react": "^16.0.35",
		"@types/react-addons-css-transition-group": "^15.0.3",
		"@types/react-dom": "^16.0.3",
		"@types/uniqid": "^4.1.2",
		"@types/window-or-global": "^1.0.0",
		"ajax-worker": "^1.8.10",
		"babel-loader": "^7.1.2",
		"deep-equal": "^1.0.1",
		"mobx": "^3.4.1",
		"raw-loader": "^0.5.1",
		"react-addons-css-transition-group": "^15.6.2",
		"uniqid": "^4.1.1",
		"utility-collection": "^1.8.26"
	}
}
